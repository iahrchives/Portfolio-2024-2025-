#include <iostream>
#include <string>
using namespace std;

int main() {
    string fullname, email, mobile, course;
    int age;

    cout << "Enter Fullname: ";
    getline(cin, fullname);
    
    cout << "Enter Email Address: ";
    getline(cin, email);
    
    cout << "Enter Mobile No: ";
    getline(cin, mobile);
    
    cout << "Enter Course: ";
    getline(cin, course);
    
    cout << "Enter Age: ";
    cin >> age;

    cout << "\nYour Name is: " << fullname << endl;
    cout << "Your Email Address is: " << email << endl;
    cout << "Your Mobile No is: " << mobile << endl;
    cout << "Your Course is: " << course << endl;
    cout << "Your Age is: " << age << endl;

    return 0;
}
